# Password hashing
Server init.

# User sets a password
>> passw := ['pass123'].

# You store it like this
>> hash := Vault 
	password-hash: passw.

# Then, user logs in
# Attempt 1
>> a1 := ['abcd123'].
# Attempt 2
>> a2 := ['pass123'].

# Verify
>> a := Vault 
	password-hash: hash verify: a1.
	# fail 
>> b := Vault
	password-hash: hash verify: a2.
	# ok!

Out write: a, stop.
Out write: b, stop.

